export default async function handler(req, res) {
    const CHANNEL_ID = "UCkaHtuBIU0JvmAxN5Qn6Ftw";

    try {
        const response = await fetch(`https://yt.lemnoslife.com/channels?part=statistics&id=${CHANNEL_ID}`);
        const data = await response.json();

        res.status(200).json(data); // <--- Esto es clave
    } catch (error) {
        console.error(error);
        res.status(500).json({ error: "Error obteniendo datos de YouTube" });
    }
}
